// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model Worker {
  id        String   @id
  name      String
  email     String   @unique
  age       Int?
  salary    Int?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
